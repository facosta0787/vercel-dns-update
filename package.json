{
  "name": "vercel-dns-update",
  "version": "0.0.1",
  "description": "small job to update vercel dns records",
  "main": "dns-update.js",
  "scripts": {
    "clean": "rm -rf ./bin",
    "rename": "mv ./bin/dns-update.js ./bin/dns-update && chmod +x ./bin/dns-update",
    "start": "",
    "build": "npm run clean && NODE_ENV=production node parcel.config.js && npm run rename",
    "webpack:build": "rm -f ./bin/dns-update* && webpack"
  },
  "keywords": [
    "node",
    "javascript",
    "vercel",
    "jobs"
  ],
  "author": "facosta0787",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "2.6.1",
    "process.argv": "0.6.0"
  },
  "devDependencies": {
    "dotenv": "^5.0.1",
    "parcel-bundler": "1.12.5",
    "parcel-plugin-shebang": "1.3.2"
  }
}
